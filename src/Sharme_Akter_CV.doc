		color: #ffffff
	}

	table.blue-cv {
		border-spacing: 0;
		border-collapse: collapse;
		width: 100%;
		border: 1px solid #DCDCDC;
		font-family: open sans, SolaimanLipi
	}

	table.blue-cv .left-side {
		background-color: #F5F5F5;
		padding: 20px 30px 20px 30px;
		width: 42%;
		vertical-align: top
	}

	table.blue-cv .right-side {
		padding: 25px 0 25px 40px;
		width: 58%;
		vertical-align: top
	}

	table.t-l-pinfo {
		border-spacing: 0;
		border-collapse: collapse;
		width: 100%;
		table-layout: auto
	}

